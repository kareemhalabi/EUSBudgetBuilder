<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="500.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1">
	<Label layoutX="14.0" layoutY="14.0" text="Welcome to EUS Budget Builder! Please note the following before proceeding:">
		<font>
			<Font name="System Bold" size="12.0" />
		</font>
	</Label>
	<Label layoutX="14.0" layoutY="31.0" text="    -  All committee files must have REV, EXP and NAME cells" />
	<Label layoutX="14.0" layoutY="48.0" text="    -  All misc porfolios must follow the template" />
	<Label layoutX="14.0" layoutY="65.0" text="    -  Revenues must be positive summed, Expenses must be summed then negated" />
	<Label layoutX="14.0" layoutY="82.0" text="    -  If using a past budget, it must have been previously been generated by this program" />
	<Label layoutX="14.0" layoutY="99.0" text="    -  Only the first sheet of every committee file, misc portfolio and past budget is read" />
	<Label layoutX="14.0" layoutY="116.0" text="    -  All files must be closed" />
